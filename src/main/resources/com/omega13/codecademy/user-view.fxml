<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.omega13.codecademy.UserController">
   <children>
      <BorderPane layoutX="-2.0" layoutY="-1.0" prefHeight="400.0" prefWidth="652.0">
         <top>
            <Label text="Cursisten" BorderPane.alignment="CENTER">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
         </top>
         <center>
            <TableView fx:id="CourseMember" prefHeight="310.0" prefWidth="652.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="Name" prefWidth="75.0" text="Naam" />
                <TableColumn fx:id="Email" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="Birthday" prefWidth="75.0" text="Geboortedatum" />
                  <TableColumn fx:id="Gender" prefWidth="75.0" text="Geslacht" />
                  <TableColumn fx:id="Address" prefWidth="75.0" text="Adres" />
                  <TableColumn fx:id="City" prefWidth="75.0" text="Woonplaats" />
                  <TableColumn fx:id="Country" prefWidth="75.0" text="Land" />
              </columns>
            </TableView>

         </center>

         <bottom>

            <Button mnemonicParsing="false" onAction="#addUser" text="Add user" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
